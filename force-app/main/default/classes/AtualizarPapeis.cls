public class AtualizarPapeis {
	public  void run() {
		try{
			Map<String, String> usuariosAlterar = this.getUsersWave1();
			usuariosAlterar.putAll(this.getUsersWave2());
			usuariosAlterar.putAll(this.getUsersWave3());
			usuariosAlterar.putAll(this.getUsersWave4());
			usuariosAlterar = this.addSuffix(usuariosAlterar);

			SET<String> keys = usuariosAlterar.keyset();



			List<User> usuarios = [select Id, Username, UserRoleId, Profile.Name from User where Username in :keys];
			Map<String, UserRole> userRoles = new Map<String, UserRole>();
			List<AggregateResult> result = [select Name, DeveloperName, Id from UserRole group by Name, DeveloperName, Id];
			for (AggregateResult r :result) {
				UserRole userRole = new UserRole();
				userRole.DeveloperName = r.get('DeveloperName').toString();
				userRole.Name = r.get('Name').toString();
				userRole.Id = r.get('Id').toString();
				userRoles.put(r.get('Name').toString(), userRole);
			}



			for (User usuario :usuarios) {
				if(!String.isBlank(usuariosAlterar.get(usuario.UserName).trim())) {
					system.debug(usuario.UserName);
					usuario.UserRoleId = userRoles.get(usuariosAlterar.get(usuario.UserName).trim()).Id;
				}
			}
			update usuarios;
		} catch(Exception e) {
			System.debug(e.getStackTraceString());
			System.debug(e.getMessage());
			System.debug(e.getLineNumber());
		}

	}

	private Map<String, String> addSuffix(Map<String, String> usuarios) {
		Map<String, String> newUsuarios = new Map<String, String>();
		for (String usuario :usuarios.keySet()) {
			if(usuario.endsWith('.nao')) {
				//Remove os caracteres '.nao'
				String usuarioSemNao = usuario.replace('.nao', '');
				newUsuarios.put(usuarioSemNao, usuarios.get(usuario).trim());
			} else {
				//newUsuarios.put(usuario + '.qalatam', usuarios.get(usuario).trim());
				newUsuarios.put(usuario + '', usuarios.get(usuario).trim());
			}

		}
		return newUsuarios;
	}

